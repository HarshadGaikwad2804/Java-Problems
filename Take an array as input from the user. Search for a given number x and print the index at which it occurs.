import java.util.*;
public class ArrayExample {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);

//Take Array size from user
		int size = sc.nextInt();

//Take user input
		int x = sc.nextInt();
		int[] num = new int[size];
		
// To store Array
		for (int i=0; i<size; i++) {
			num[i] = sc.nextInt(); 
		}

//print the array
		for (int j=0; j<num.length; j++) {

// to search the user input is in the list or not
			if (num[j] == x) {
				System.out.println(x + " This number is matching with user input");
				continue;
			}
			else {
				System.out.println(num[j]);
			}	
		}
	}
}
